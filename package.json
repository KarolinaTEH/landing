{
  "name": "landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "tunnel": "ngrok http --url=legible-key-frog.ngrok-free.app 3000",
    "dev:en": "cross-env NEXT_PUBLIC_LOCALE=en next dev",
    "dev:de": "cross-env NEXT_PUBLIC_LOCALE=de next dev",
    "dev:pl": "cross-env NEXT_PUBLIC_LOCALE=pl next dev",
    "fetch-translations-local": "env-cmd -f .env.local tsx scripts/fetch-translations.ts",
    "fetch-translations": "tsx scripts/fetch-translations.ts",
    "build": "npm run fetch-translations &&next build",
    "build-local": "npm run fetch-translations-local && next build",
    "start": "next start",
    "lint": "next lint",
    "shadcn": "for file in components/ui/*.tsx; do npx shadcn@canary add -y -o $(basename $file .tsx); done",
    "update-kysely-types": "env-cmd -f .env.local kysely-codegen --dialect=postgres --out-file ./db/kysely-types.ts --exclude-pattern spatial_ref_sys",
    "migrate": "env-cmd -f .env.local tsx scripts/migrate.ts"
  },
